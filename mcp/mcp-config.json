{
  "mcpServers" : {
    "context7" : {
      "command" : "npx",
      "args" : [
        "-y",
        "@upstash\/context7-mcp@latest"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"]
    },
    "linear": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.linear.app/mcp"]
    }
  }
}
