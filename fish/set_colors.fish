set fish_color_normal FCFCFA # the default color
set fish_color_command FCFCFA # the color for commands
set fish_color_quote FFD866 # the color for quoted blocks of text
set fish_color_redirection A9DC76 # the color for IO redirections
set fish_color_end 727072 # the color for process separators like ';' and '&'
set fish_color_error FCFCFA --background=FC9867 # the color used to highlight potential errors
set fish_color_param AB9DF2 # the color for regular command parameters
set fish_color_comment 727072 # the color used for code comments
set fish_color_match FF6188 # the color used to highlight matching parenthesis
set fish_color_selection 434144 # the color used when selecting text (in vi visual mode)
set fish_color_search_match --background=4C4A4C # used to highlight history search matches and the selected pager item (must be a background)
set fish_color_operator A9DC76 # the color for parameter expansion operators like '*' and '~'
set fish_color_escape 78DCE8 # the color used to highlight character escapes like '\n' and '\x70'
set fish_color_cwd 78DCE8 # the color used for the current working directory in the default prompt
set fish_color_autosuggestion 727072 # the color used for autosuggestions
set fish_color_user FFD866 # the color used to print the current username in some of fish default prompts
set fish_color_host FFD866 #the color used to print the current host system in some of fish default prompts
set fish_color_host_remote FC9867 # the color used to print the current host system in some of fish default prompts, if fish is running remotely (via ssh or similar)
set fish_color_cancel FCFCFA # the color for the '^C' indicator on a canceled command

# Additionally, the following variables are available to change the highlighting in the completion pager:
set fish_pager_color_progress A9DC76 # the color of the progress bar at the bottom left corner
set fish_pager_color_background FFD866 # the background color of a line
set fish_pager_color_prefix FCFCFA # the color of the prefix string, i.e. the string that is to be completed
set fish_pager_color_completion FCFCFA # the color of the completion itself
set fish_pager_color_description FC9867 # the color of the completion description
# set fish_pager_color_secondary_background # fish_pager_color_background of every second unselected completion. Defaults to fish_pager_color_background
# set fish_pager_color_secondary_prefix # fish_pager_color_prefix of every second unselected completion. Defaults to fish_pager_color_prefix
# set fish_pager_color_secondary_completion # fish_pager_color_completion of every second unselected completion. Defaults to fish_pager_color_completion
# set fish_pager_color_secondary_description # fish_pager_color_description of every second unselected completion. Defaults to fish_pager_color_description
# set fish_pager_color_selected_background # fish_pager_color_background of the selected completion. Defaults to fish_color_search_match
# set fish_pager_color_selected_prefix # fish_pager_color_prefix of the selected completion. Defaults to fish_pager_color_prefix
# set fish_pager_color_selected_completion # fish_pager_color_completion of the selected completion. Defaults to fish_pager_color_completion
# set fish_pager_color_selected_description # fish_pager_color_description of the selected completion. Defaults to fish_pager_color_description